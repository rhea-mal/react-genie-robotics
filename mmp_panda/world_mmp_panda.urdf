<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 -3.0">

	<robot name="mmp_panda">
		<model dir="${CS225A_URDF_FOLDER}/mmp_panda" path="mmp_panda.urdf" name="mmp_panda" />
	</robot>


	<robot name="toro">
		<model dir="${CS225A_URDF_FOLDER}/toro" path="toro.urdf" name="toro" />
		<origin xyz="2 0 -0.94" rpy="0 0 0" />
	</robot>


	<static_object name="Base">
		<origin xyz="0.0 0.0 -0.35" rpy="0 0 0" />
	    <visual>
	        <origin xyz="0.0 0.0 -0.00635" rpy="0 0 0" />
	        <geometry>
				<box size="55 55 0.0127" />
	        </geometry>
	        <material name="material">
		        <color rgba="0.1 0.1 0.1 1.0" />
	        </material>
	    </visual>
		<collision>
	        <origin xyz="0.0 0.0 -0.00635" rpy="0 0 0" />
	        <geometry>
	            <box size="55 55 0.0127" />
	        </geometry>
	    </collision>
	</static_object>

	<dynamic_object name="cup">
		<origin xyz="-2.0 0.0 1.0" rpy="0 0 0" />
		<inertial>
        	<origin xyz="0 0 0" rpy="0 0 0" />
        	<mass value="10" />
        	<inertia ixx="1" iyy="1" izz="1" ixy="0" ixz="0" iyz="0" />
      	</inertial>

	    <visual>
	        <origin xyz="0 0 0.45" rpy="0 0 0" />
			<geometry>
	            <mesh filename="${CS225A_URDF_FOLDER}/test_objects/meshes/visual/cup.obj" />
	        </geometry>
	    </visual>

	    <collision>
	        <origin xyz="0 0 0.06" rpy="0 0 0" />
	        <geometry>
	            <box size="0.01 0.01 0.01" />
	        </geometry>
	    </collision>
	</dynamic_object>


	<dynamic_object name="ball">
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
		<inertial>
        	<origin xyz="0 0 0" rpy="0 0 0" />
        	<mass value="1.0" />
        	<inertia ixx="1" iyy="1" izz="1" ixy="0" ixz="0" iyz="0" />
      	</inertial>

	    <visual>
	        <origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
	            <sphere radius="0.1" />
	        </geometry>
			<material name="material">
				<color rgba = "1 0 0 0" />
			</material>
	    </visual>

	    <collision>
	        <origin xyz="0 0 0" rpy="0 0 0" />
	        <geometry>
	            <sphere radius="0.1" />
	        </geometry>
	    </collision>
	</dynamic_object>

	<light name="light1" type="directional">
		<position xyz="2.0 2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<light name="light2" type="directional">
		<position xyz="-2.0 -2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<camera name="camera_fixed">
		<position xyz="2.0 -0.8 6.0" />
		<vertical xyz="0.0 0.0 20.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</camera>

</world>
